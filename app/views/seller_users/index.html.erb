<p id="notice"><%= notice %></p>

<h1>Listing Seller Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seller_users.each do |seller_user| %>
      <tr>
        <td><%= link_to 'Show', seller_user %></td>
        <td><%= link_to 'Edit', edit_seller_user_path(seller_user) %></td>
        <td><%= link_to 'Destroy', seller_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller user', new_seller_user_path %>
