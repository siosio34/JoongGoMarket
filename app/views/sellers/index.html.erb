<p id="notice"><%= notice %></p>

<h1>Listing Sellers</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Sellcount</th>
      <th>Profit</th>
      <th>Grade</th>
      <th>Phonenumber</th>
      <th>Intro</th>
      <th>Bank</th>
      <th>Banknumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sellers.each do |seller| %>
      <tr>
        <td><%= seller.customer %></td>
        <td><%= seller.sellcount %></td>
        <td><%= seller.profit %></td>
        <td><%= seller.grade %></td>
        <td><%= seller.phonenumber %></td>
        <td><%= seller.intro %></td>
        <td><%= seller.bank %></td>
        <td><%= seller.banknumber %></td>
        <td><%= link_to 'Show', seller %></td>
        <td><%= link_to 'Edit', edit_seller_path(seller) %></td>
        <td><%= link_to 'Destroy', seller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seller', new_seller_path %>
